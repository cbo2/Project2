<div class="container">
  <div class="jumbotron mt-4" id="jmbo">
     <h1 class="display-4 center">{{msg}}</h1>
    <p class="lead center">{{title}}</p>
   
    <div class="col-sm">
      <div id="logbox">
        <form id="account" method="POST" action="/profile">
          <h2>Create Account</h2>
          <input name="user[name]" type="text" id="userName" placeholder="User Name" class="input pass" />
          <input name="user[password]" id="password" type="password" placeholder="Enter Password" required="required" class="input pass" />
          <input type="submit" value="Create Account!" class="inputButton" id="newAcc" />

        </form>
      </div>
    </div>
    <a href="/">‚Üê Back To Home</a>
  </div>
</div>



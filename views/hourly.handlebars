<div id="jmbo" class="jumbotron jumbotron-fluid">
    <div class="container">
        <h1 class="display-4 center">Hourly Weahter Data For {{user.username}}</h1>
    </div>
</div>

<div class="container">
    <div class="card text-center" id="profileContainer">
        <div class="card-header">
            <h3 class="panel-title"></h3>

            <div class="row">
                <div class="col">

                </div>
                <div class=" col-md-9 col-lg-9 ">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th scope="col">Hour</th>
                                <th scope="col">Summary</th>
                                <th scope="col">Temp</th>
                                <th scope="col">Humidity</th>
                                <th scope="col">Wind Speed</th>
                                <th scope="col">Wind Gust</th>
                                <th scope="col">Icon</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each hourly}}
                            <tr>
                                <th scope="row">{{@index}}</th>
                                <td>{{this.temperature}}</td>
                                <td>{{this.summary}}</td>
                                <td>{{this.temperature}}</td>
                                <td>{{this.humidity}}</td>
                                <td>{{this.windSpeed}}</td>
                                <td>{{this.windGust}}</td>
                                <td>{{this.icon}}</td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
                <div class="col">

                </div>
            </div>
        </div>
    </div>





